#!/bin/bash

# do not edit this file.
# customize values in custom.env

ZOOKEEPER_OPERATOR_NAMESPACE="instana-zookeeper"
KAFKA_NAMESPACE="instana-kafka"
ELASTIC_NAMESPACE="instana-elastic"
POSTGRES_NAMESPACE="instana-postgres"
CASSANDRA_NAMESPACE="instana-cassandra"
CLICKHOUSE_NAMESPACE="instana-clickhouse"
BEEINSTANA_NAMESPACE="beeinstana"
INSTANA_OPERATOR_NAMESPACE="instana-operator"
INSTANA_CORE_NAMESPACE="instana-core"
INSTANA_UNIT_NAMESPACE="instana-units"

# do not customize release names
ZOOKEEPER_HELM_RELEASE="instana"
KAFKA_HELM_RELEASE="strimzi"
ELASTIC_HELM_RELEASE="elastic-operator"
POSTGRES_HELM_RELEASE="postgres-operator"
CASSANDRA_HELM_RELEASE="cass-operator"
CLICKHOUSE_HELM_RELEASE="clickhouse-operator"
BEEINSTANA_HELM_RELEASE="beeinstana"

# set in custom.evn
INSTANA_DOWNLOAD_KEY="instana-download-key"
INSTANA_SALES_KEY="instana-sales-key"

# kubectl instana plugin
KUBECTL=oc
KUBECTL_INSTANA_OS=darwin
KUBECTL_INSTANA_ARCH=amd64

INSTANA_CORE_TOKEN_SECRET="123456789012"
INSTANA_CORE_PEM_KEY_PASSWORD="password"

# clickhouse
CLICKHOUSE_IMAGE_SECRET="clickhouse-image-secret"

CLICKHOUSE_ADMIN="default"
CLICKHOUSE_ADMIN_PASSWORD="clickhouse-default-pass"
CLICKHOUSE_USER="clickhouse-user"
CLICKHOUSE_USER_PASSWORD="clickhouse-pass"

RWX_STORAGECLASS="ocs-storagecluster-cephfs"

INSTANA_ADMIN_USER="admin@instana.local"
INSTANA_ADMIN_PASSWORD="password"

INSTANA_UNIT_NAME=unit0
INSTANA_TENANT_NAME=tenant0
INSTANA_BASE_DOMAIN="instana.base.domain"

# $instana_unit_name-$instana_tenant_name.$instana_base_domain
INSTANA_TENANT_DOMAIN=unit0-tenant0.instana_base_domain

# agents.$instana_base_domain
INSTANA_AGENT_ACCEPTOR="agents.instana.base.domain"

# use base64 < /dev/urandom | head -c32 to generate strong password
BEEINSTANA_ADMIN_PASSWORD="password"
